    "build:web": {
      "name": "build:web",
      "command": "yarn build:web",
      "runAtStart": false
    },
    "build:desktop": {
      "name": "build:desktop",
      "command": "yarn build:desktop",
      "runAtStart": false
    },
    "build:storybook": {
      "name": "build:storybook",
      "command": "yarn build:storybook",
      "runAtStart": false
    },
    "build:tokens": {
      "name": "build:tokens",
      "command": "yarn build:tokens",
