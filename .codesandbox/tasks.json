    "prepare": {
      "name": "prepare",
      "command": "yarn prepare",
      "runAtStart": false
    },
    "dev:web": {
      "name": "dev:web",
      "command": "yarn dev:web",
      "runAtStart": false
    },
    "dev:desktop": {
      "name": "dev:desktop",
      "command": "yarn dev:desktop",
      "runAtStart": false
    },
    "storybook": {
      "name": "storybook",
      "command": "yarn storybook",
      "runAtStart": false
